-- display songs from sweetener world tour setlist with corresponding album name/year
SELECT sweetener_setlist.song AS SWT_setlist, agd.album, agd.year
FROM ariana_grande_discography AS agd 
JOIN sweetener_setlist
ON agd.id=sweetener_setlist.song_id;


-- display songs from sweetener world tour setlist released in 2018 in alphabetical order 
SELECT sweetener_setlist.song AS SWT_setlist, agd.album, agd.year
FROM ariana_grande_discography AS agd 
JOIN sweetener_setlist 
ON agd.id=sweetener_setlist.song_id
WHERE agd.year = 2018 
ORDER BY SWT_setlist


